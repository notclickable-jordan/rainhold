# Caddyfile for notclickable.com stack

# Root domain: reverse proxy to not-clickable-redirect
notclickable.com {
    reverse_proxy localhost:9002
}

www.notclickable.com {
    reverse_proxy localhost:9002
}

# Authentik: reverse proxy to Authentik service
auth.notclickable.com {
    reverse_proxy localhost:9000
}