# Root domain
notclickable.com {
    reverse_proxy localhost:9002
}

www.notclickable.com {
    reverse_proxy localhost:9002
}

dozzle.notclickable.com {
    reverse_proxy https://localhost:9001
}

# Authentik
auth.notclickable.com {
    reverse_proxy https://localhost:9443 {
        transport http {
            tls_insecure_skip_verify
        }
    }
}